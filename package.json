    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@tsconfig/esm": "^1.0.3",
    "@tsconfig/node-lts": "^18.12.1",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^20.1.0",
    "@vitest/ui": "^0.31.0",
    "fast-toml": "^0.5.4",
    "rollup": "^3.28.1",
    "tslib": "^2.6.2",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
